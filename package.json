{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "CRUD API using in-memory database underneath",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --watch src src/index.ts",
    "start:multi": "cross-env BALANCER_MODE=multi npm run start:dev",
    "start:prod": "npm run build && node ./dist/bundle.js",
    "start:prod:multi": "npm run build && cross-env BALANCER_MODE=multi node ./dist/bundle.js",
    "build": "webpack --node-env=production",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksryab/crud-api.git"
  },
  "keywords": [
    "node.js",
    "CRUD"
  ],
  "author": "aleksryab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aleksryab/crud-api/issues"
  },
  "homepage": "https://github.com/aleksryab/crud-api#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
}
